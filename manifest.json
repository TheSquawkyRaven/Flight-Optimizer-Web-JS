{
	"name": "Flight Optimizer",
	"description": "Made by Raven",
	"version": "1.0",
	"permissions": [
		"activeTab",
		"cookies",
		"storage",
		"scripting"
	],
	"action": {
		"default_popup": "popup.html"
	},
	"icons": {
		"16": "icon.png"
	},
	"host_permissions": [
		"https://www.google.com/travel/flights/search"
	],
	"manifest_version": 3
}